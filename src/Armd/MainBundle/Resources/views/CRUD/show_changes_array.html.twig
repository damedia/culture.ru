{% extends 'SonataAdminBundle:CRUD:base_show_field.html.twig' %}

{% block field%}
    <table>
    {% for key, val in value %}
        <tr>
            <th colspan="3">{{ key }}</th>
        </tr>
        <tr>
            <td>{{ val.0|raw }}</td>
            <td> -> </td>
            <td>{{ val.1|raw }}</td>
        </tr>
    {% endfor %}
    </table>
{% endblock %}
