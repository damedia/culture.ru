	$(function(){
		/*Gov*/
		$('.gos-buttons a').click(function(){
			var blockId = $(this).parent('li').attr('id');
			$(this).parent('li').addClass('active').siblings().removeClass('active');
			$('.gos-list li#'+blockId+'_block').show().siblings().hide();
			$('.gos-prims').show();
		})
	})
</script>
<script type="text/javascript">
  $(function () {
      initUi();
  });

  function initUi() {
      $('#show-services').parent().parent().click(function(){
          $('#services').parent().removeClass('collapsed');
          $(this).hide();
          return false;
      });

      $('#show-regions').parent().parent().click(function(){
          $('#regions').parent().removeClass('collapsed');
          $(this).hide();
          return false;
      });
  }
</script>
