    <script type="text/javascript" src="{{ asset('bundles/armdlecture/js/lectures.js') }}"></script>
    <script type="text/javascript">
        $(function() {
           {# armdMkLectures.init('{{ lectureSuperType.code }}'); #}
           
           function initFancybox() {
            $('.fancybox').fancybox({
                autoResize: true,
                prevEffect: 'none',
		        nextEffect: 'none'
            });

            $('.zoom-fancybox').fancybox({
                 autoResize:true,
                 afterShow:function ()
                 {
                     var innerWidth = $('.fancybox-inner').width();
                     var innerHeight = $('.fancybox-inner').height();

                     var newContent = $('<a id="zoom" href="/uploads/media/atlas/0001/09/a3dd55e8a1be10487f3a21af855e67072373beae.jpeg"><img class="zoom-img" src="/uploads/media/atlas/0001/09/a3dd55e8a1be10487f3a21af855e67072373beae.jpeg" height="' + innerHeight + '"></img></a><div class="info-line"><a href="/uploads/media/atlas/0001/09/a3dd55e8a1be10487f3a21af855e67072373beae.jpeg" target="_blank">Скачать</a><a href="/uploads/media/atlas/0001/09/a3dd55e8a1be10487f3a21af855e67072373beae.jpeg" id="fancy_print">Распечатать</a></div>');
                     $('.fancybox-inner').html(newContent);
                     $('#zoom').jqzoom({
                         zoomType:'innerzoom',
                         lens:false,
                         title:false,
                         preloadImages:false,
                         alwaysOn:false
                     })

                     $('#fancy_print').bind("click", function ()
                     {
                         var win = null;
                         var content = $('#zoom');
                         win = window.open("width=200,height=200");
                         self.focus();
                         win.document.open();
                         win.document.write('<' + 'html' + '><' + 'head' + '><' + 'style' + '>');
                         win.document.write('body, td { font-family: Verdana; font-size: 10pt;}');
                         win.document.write('<' + '/' + 'style' + '><' + '/' + 'head' + '><' + 'body' + '>');
                         win.document.write(content.html());
                         win.document.write('<' + '/' + 'body' + '><' + '/' + 'html' + '>');
                         win.document.close();
                         win.print();
                         win.close();

                         return false;
                     }); // bind

                 }
             });
            }
            initFancybox();
           
        });
    </script>
{% endblock %}
