<table border="1px">
    <tr>
        <td><a id="calendar-prev" href="#">prev</a></td>
        <td colspan="5">{{ calendar.months.current }}</td>
        <td><a id="calendar-next" href="#">next</a></td>
    </tr>
    <tr>
        <th>Пн</th>
        <th>Вт</th>
        <th>Ср</th>
        <th>Чт</th>
        <th>Пт</th>
        <th class="weekend">Сб</th>
        <th class="weekend">Вс</th>
    </tr>
{% for week in calendar.weeks %}
    <tr>
    {% for day in week %}
        <td {% if day.isWeekend %} class="weekend" {% endif %}>
            <a href="{{ path('armd_news_news_archive_1', {'year': calendar.years.current, 'month': '%02d'|format(calendar.months.current), 'day': '%02d'|format(day.day)}) }}">{{ day.day }}</a>
        </td>
    {% endfor %}
    </tr>
{% endfor %}    
</table>
<a href="{{ path('armd_news_news_archive', {month: 6}) }}">!!</a>