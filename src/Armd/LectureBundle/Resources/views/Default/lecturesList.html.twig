{% set counter = 1 %}

{% spaceless %}
    {% for object in lectures %}
        {% if counter <= 4 %}
            {% include 'ArmdLectureBundle:Default:lecturesList_item.html.twig' with {'item': object} %}
        {% elseif counter > 4 and counter <= 10 %}
            {% if counter == 5 %}
                <div class="lectures-list-item-6pack">
            {% endif %}

            {% include 'ArmdLectureBundle:Default:lecturesList_item.html.twig' with {'item': object} %}

            {% if counter == 10 %}
                </div>
            {% endif %}
        {% elseif counter == 11 %}
            {% include 'ArmdLectureBundle:Default:lecturesList_itemBig.html.twig' with {'item': object} %}
        {% endif %}

        {% set counter = counter + 1 %}
    {% endfor %}
{% endspaceless %}