{% set counter = 1 %}

{% spaceless %}
    {% for object in objects %}
        {% if counter <= 4 %}
            {% include 'ArmdTheaterBundle:Default:theaters_list_item.html.twig' with {'item': object} %}
        {% elseif counter > 4 and counter <= 8 %}
            {% if counter == 5 %}
                <div class="imageOfRussia-list-item-4pack">
            {% endif %}

            {% include 'ArmdTheaterBundle:Default:theaters_list_item.html.twig' with {'item': object} %}

            {% if counter == 8 %}
                </div>
            {% endif %}
        {% elseif counter == 9 %}
            {% include 'ArmdTheaterBundle:Default:theaters_list_item_big.html.twig' with {'item': object} %}
        {% else %}
            {% include 'ArmdTheaterBundle:Default:theaters_list_item.html.twig' with {'item': object} %}
        {% endif %}

        {% set counter = counter + 1 %}
    {% endfor %}
{% endspaceless %}