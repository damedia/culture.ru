{% extends ':Frontend:layout_two_column.html.twig' %}

{% block stylesheets %}
    {{ parent() }}

    <style type="text/css">
        {{ Stylesheet|raw }}
    </style>
{% endblock %}

{% block title %}
    {{ parent() }} - {{ PageTitle }}
{% endblock %}

{% block page_header_title %}
    {{ PageTitle }}
{% endblock %}

{% block left_column %}
	{% block breadcrumbs %}
        {{ include('DamediaSpecialProjectBundle:Default:breadcrumbs.html.twig', { 'Breadcrumbs': Breadcrumbs }) }}
	{% endblock %}

    {{ Blocks.LeftBlock|raw }}
{% endblock %}

{% block right_column %}
    {{ Blocks.RightBlock|raw }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}

    <script type="text/javascript">
        {{ Javascript|raw }}
    </script>
{% endblock %}